{
    "start_url": "https://es.m.wikipedia.org/wiki/Pueblos_ind%C3%ADgenas_de_M%C3%A9xico",
    "actions": [
        {
            "action": "wait",
            "wait": 3
        },
        {
            "action": "find_element_table",
            "by": "CSS_SELECTOR",
            "value": "table.sortable.striped.col3der.jquery-tablesorter"
        },
        {
            "action": "find_elements_table_rows",
            "by": "TAG_NAME",
            "value": "tr",
            "actions": [
                {
                    "action": "find_elements_table_columns",
                    "by": "TAG_NAME",
                    "value": "th",
                    "save_as": "headers"
                },
                {
                    "action": "get_element_text_list",
                    "by": "TAG_NAME",
                    "value": "td",
                    "save_as": "rows"
                }
            ]
        }
    ],
    "output": {
        "filename": "poblacion_indigena_mexico.xlsx"
    }
}
